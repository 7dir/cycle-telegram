{
  "name": "cycle-telegram",
  "version": "0.0.0-placeholder",
  "typings": "index.d.ts",
  "description": "CycleJS Telegram Driver",
  "main": "index.js",
  "keywords": [
    "cyclejs",
    "telegram"
  ],
  "directories": {},
  "publishConfig": {
    "tag": "dev"
  },
  "release": {
    "branch": "development"
  },
  "scripts": {
    "clean": "rm -rf lib/ && mkdir -p lib && typings install",
    "build": "babel -d lib/ src/",
    "build:watch": "babel -w -d lib/ src/",
    "eslint": "eslint src/",
    "test": "node -r babel-register test | tap-spec",
    "test:record": "node -r babel-register test/integration/record.js | tap-spec",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "goodmind <andwebar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cycle/isolate": "^1.3.2",
    "ramda": "^0.21.0",
    "rx": "^4.1.0",
    "superagent": "^2.0.0-alpha.3",
    "tcomb": "^3.2.6"
  },
  "devDependencies": {
    "@cycle/core": "^6.0.3",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "semantic-release": "^4.3.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-nock": "^1.2.1",
    "typings": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodmind/cycle-telegram"
  }
}
