{
  "name": "cycle-telegram",
  "version": "1.1.0",
  "description": "CycleJS Telegram Driver",
  "main": "index.js",
  "keywords": [
    "cyclejs",
    "telegram"
  ],
  "directories": {},
  "scripts": {
    "clean": "rm -rf lib/ && mkdir -p lib",
    "build": "babel -d lib/ src/",
    "build:watch": "babel -w -d lib/ src/",
    "eslint": "eslint src/ && eslint example/",
    "test": "node -r babel-register test | tap-spec",
    "test:record": "node -r babel-register test/record.js | tap-spec",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "goodmind <andwebar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "node-uuid": "^1.4.7",
    "ramda": "^0.21.0",
    "rx": "^4.1.0",
    "superagent": "^2.0.0-alpha.3",
    "tcomb": "^3.0.0"
  },
  "devDependencies": {
    "@cycle/core": "^6.0.3",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-nock": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodmind/cycle-telegram"
  }
}
